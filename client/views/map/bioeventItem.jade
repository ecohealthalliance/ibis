template(name='bioeventItem')
  li.bioevent-item
    h6
      a(href="/bioevents/#{bioevent.event._id}")=bioevent.event.name
      if bioevent.rank
        .rank-score.rank-data Rank Score: #{bioevent.rank}
      else if bioevent.lastIncident
        .rank-data Last Incident: #{bioevent.lastIncident}
      else if bioevent.activeCases
        .rank-data Estimated Active Cases: #{bioevent.activeCases}
    #[+timeline dateRange=dateRange max=timelineMax resolvedBioevent=bioevent.event]#[.minimap]
